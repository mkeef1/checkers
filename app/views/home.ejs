<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
  </head>
  <body>
    <div>
      <table>
        <% var bg, getsPiece = false, url %>
        <% for(var i = 1; i < 9; i++){ %>
          <tr class="tr<%= i % 2%>">
            <% for(var j = 1; j < 9; j++) {%>
              <% if(i%2===1){ %>
                <% bg = (j%2===1) ? 'white' : 'black' %>
              <% }else{ %>
                <% bg = (j%2===1) ? 'black' : 'white' %>
              <% } %>
              <td class="td<%= j%2 %>"></td>
            <% } %>
          </tr>
        <% } %>
      </table>
    </div>  
  </body>
</html>
